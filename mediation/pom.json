{
   "@schemaLocation": "http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd",
   "modelVersion": "4.0.0",
   "groupId": "org.wso2.carbon",
   "artifactId": "edu.byu.wso2.apim.extensions",
   "version": "2.0.0",
   "packaging": "jar",
   "name": "BYU Custom APIM Mediators",
   "repositories": {
      "repository": {
         "id": "wso2-nexus",
         "name": "WSO2 internal Repository",
         "url": "http://maven.wso2.org/nexus/content/groups/wso2-public/",
         "releases": {
            "enabled": "true",
            "updatePolicy": "daily",
            "checksumPolicy": "warn"
         }
      }
   },
   "url": "http://maven.apache.org",
   "build": {
      "plugins": {
         "plugin": {
            "groupId": "org.apache.maven.plugins",
            "artifactId": "maven-compiler-plugin",
            "version": "2.3.1",
            "configuration": {
               "source": "1.7",
               "target": "1.7"
            }
         }
      }
   },
   "properties": {
      "synapse.version": "2.1.7-wso2v10",
      "carbon.kernel.version": "4.4.11",
      "carbon.apimgt.version": "6.1.66",
      "httpcomponents.version": "4.2.5"
   },
   "dependencies": {
      "dependency": [
         {
            "groupId": "org.apache.synapse",
            "artifactId": "synapse-core",
            "version": "${synapse.version}"
         },
         {
            "groupId": "org.wso2.carbon",
            "artifactId": "org.wso2.carbon.utils",
            "version": "${carbon.kernel.version}"
         },
         {
            "groupId": "org.wso2.carbon.apimgt",
            "artifactId": "org.wso2.carbon.apimgt.gateway",
            "version": "${carbon.apimgt.version}"
         },
         {
            "groupId": "org.wso2.carbon.apimgt",
            "artifactId": "org.wso2.carbon.apimgt.api",
            "version": "${carbon.apimgt.version}"
         },
         {
            "groupId": "org.wso2.carbon.apimgt",
            "artifactId": "org.wso2.carbon.apimgt.impl",
            "version": "${carbon.apimgt.version}"
         },
         {
            "groupId": "edu.byu.wso2",
            "artifactId": "EntityDAO",
            "version": "1.0.0-SNAPSHOT"
         }
      ]
   }
}